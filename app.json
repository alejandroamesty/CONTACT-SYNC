{
  "expo": {
    "name": "Contact Sync",
    "slug": "contact-sync",
    "version": "1.0.0",
    "orientation": "portrait",
    "assetBundlePatterns": [
      "**/*"
    ],
    "icon": "./assets/images/contact-sync-icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/contact-sync-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#030B38"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/contact-sync-adaptive-icon.png",
        "backgroundColor": "#030B38"
      },
      "package": "com.alejandroamesty.contactsync",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          },
          "ios": {}
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "9429c956-ab90-4053-9c43-33a585f3b0d6"
      }
    }
  }
}
